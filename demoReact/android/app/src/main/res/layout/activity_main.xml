<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

        <Button
            android:id="@+id/btnStartReactNative"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Start React Native"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <Button
            android:id="@+id/btnStartFlutterMain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Start Flutter Main"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnStartReactNative" />
        <Button
            android:id="@+id/btnStartFlutterSecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Start Flutter Second"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnStartFlutterMain" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>